{
    "name": "cakebot",
    "version": "2.0.0",
    "description": "A new Discord bot with fun and helpful features.",
    "keywords": [
        "discord",
        "bot"
    ],
    "homepage": "https://cakebot.club",
    "bugs": {
        "url": "https://github.com/cakebotpro/cakebot/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/cakebotpro/cakebot.git"
    },
    "license": "AGPL-3.0",
    "author": "Reece Dunham <me@rdil.rocks>",
    "main": "build/index.js",
    "types": "build/index.d.ts",
    "bin": "build/cli.js",
    "directories": {
        "lib": "build"
    },
    "files": [
        "build",
        ".github/SECURITY.md",
        "content"
    ],
    "scripts": {
        "build": "rimraf build && tsc",
        "lint": "eslint src/**/**/*.ts src/**/*.ts src/*.ts --fix",
        "prettier": "prettier --write **/*.{js,ts,md,yml,json}",
        "start": "rimraf build && tsc && node launcher.js"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.{js,ts,json,yml,md}": "prettier --write"
    },
    "prettier": {
        "semi": false,
        "tabWidth": 4
    },
    "dependencies": {
        "@octokit/core": "^3.2.5",
        "chalk": "^4.1.0",
        "csv-parse": "^4.15.0",
        "discord.js": "^12.5.1",
        "dotenv": "^8.2.0",
        "kdt": "^0.1.0",
        "node-fetch": "^2.6.1",
        "pirates": "^4.0.1",
        "random": "^2.2.0",
        "source-map-support": "^0.5.19",
        "sync-fetch": "^0.3.0",
        "tslib": "^2.1.0"
    },
    "devDependencies": {
        "@types/node": "^14.14.22",
        "@types/node-fetch": "^2.5.8",
        "@types/random": "^2.1.1",
        "@types/source-map-support": "^0.5.3",
        "@types/ws": "^7.4.0",
        "@typescript-eslint/eslint-plugin": "^4.14.1",
        "@typescript-eslint/parser": "^4.14.1",
        "eslint": "^7.18.0",
        "eslint-plugin-header": "^3.1.0",
        "eslint-plugin-promise": "^4.2.1",
        "husky": "^4.3.8",
        "lint-staged": "^10.5.3",
        "prettier": "^2.2.1",
        "prettier-plugin-organize-imports": "^1.1.1",
        "prettier-plugin-packagejson": "^2.2.9",
        "rimraf": "^3.0.2",
        "typescript": "^4.1.3"
    },
    "optionalDependencies": {
        "bufferutil": "^4.0.3",
        "erlpack": "^0.1.3",
        "utf-8-validate": "^5.0.4",
        "zlib-sync": "^0.1.7"
    },
    "engines": {
        "node": ">=12.13",
        "yarn": ">=1.17"
    }
}
